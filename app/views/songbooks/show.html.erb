<h1><%= @songbook.name %></h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Created</th>
    </tr>
  </thead>
  <tbody>
    <% @songbook.songs.each do |song| %>
      <tr><td><%= song.artist %></td><td><%= song.title %></td></tr>
    <% end %>
  </tbody>
</table>
